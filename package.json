{
  "name": "scully-multi-markdown-page-builder-plugin-homepage",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --stats-json && npm run build:plugin && npm run build:apidoc && npm run scully -- --scanRoutes",
    "build:serve": "npm run build && npm run scully serve",
    "build:prod": "ng build --stats-json --prod && npm run build:plugin:prod && npm run build:apidoc && npm run scully -- --scanRoutes --prod",
    "build:prod:serve": "npm run build:prod && npm run scully serve",
    "build:plugin": "ng build scully-multi-markdown-page-builder-plugin",
    "build:plugin:prod": "ng build scully-multi-markdown-page-builder-plugin --prod",
    "build:apidoc": "jsdoc2md --files ./projects/scully-multi-markdown-page-builder-plugin/src/lib/*.ts --configure ./jsdoc2md.json > ./docs/api.md && echo \"---\ntitle: API Docs\n---\n$(cat ./docs/api.md)\" > ./docs/api.md",
    "publish": "gh-pages --dotfiles --message '[skip ci] Lastest distribution' --dist dist/static",
    "test": "ng test",
    "test:jest": "jest --runInBand --detectOpenHandles src/",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@scullyio/init": "^1.0.0-beta.0",
    "@scullyio/ng-lib": "^1.0.0-beta.0",
    "@scullyio/scully": "^1.0.0-beta.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "jsdom": "^16.3.0",
    "marked": "^1.1.1",
    "@types/marked": "^1.1.0",
    "highlight.js": "^10.1.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.0",
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@scullyio/scully-plugin-base-href-rewrite": "^1.0.0-beta.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.2.2",
    "jest-preset-angular": "^8.2.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "ng-packagr": "^10.1.1",
    "protractor": "~7.0.0",
    "scully-plugin-disable-angular": "^5.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^3.8.3"
  }
}
